    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "chai": "^3.4.1",
    "eslint": "^4.5.0",
    "eslint-config-metalab": "^7.0.1",
    "flow-bin": "^0.56.0",
    "mocha": "^2.3.4",
